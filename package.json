{
  "name": "react-lazylog",
  "version": "1.0.1",
  "description": "React Lazy Logviewer",
  "license": "MPL-2.0",
  "repository": "mozilla-rpweb/react-lazylog",
  "main": "lib/LazyLog.js",
  "files": [
    "lib",
    "src/components/LazyLog"
  ],
  "keywords": [
    "react",
